{
    "name": "ae/connect-bundle",
    "description": "Synchronize Doctrine Entities to and from one or more Salesforce Orgs",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/advisors-excel-llc/salesforce-rest-sdk.git"
        }
    ],
    "require": {
        "ext-pcntl": "*",
        "symfony/framework-bundle": "~3.4.10|^4.1",
        "doctrine/doctrine-bundle": "^1.9",
        "ramsey/uuid-doctrine": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "jms/serializer-bundle": "^2.4",
        "ae/salesforce-rest-sdk": "^1.3",
        "doctrine/doctrine-cache-bundle": "^1.3",
        "enqueue/enqueue-bundle": "^0.9",
        "symfony/validator": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "symfony/phpunit-bridge": "~3.4.10|^4.1",
        "theofidry/alice-data-fixtures": "^1.0",
        "symfony/monolog-bundle": "^3.3",
        "ext-pdo_sqlite": "*",
        "matthiasnoback/symfony-config-test": "^4.0",
        "enqueue/fs": "^0.9",
        "doctrine/data-fixtures": "^1.3"
    },
    "authors": [
        {
            "name": "Alex Boyce",
            "email": "alex.boyce@advisorsexcel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AE\\ConnectBundle\\": ""
        }
    }
}
